<h1>Scrape Menu</h1>


<div id="location_select">
        <% count = 0 %>
        <% LocationUrlMap.find(:all,:order=>'static_location').each do |location_row| %>
            <div class="location_link">
                <%= link_to location_row.location_string, "#{root_url}scrape/#{location_row.static_location}/", {:target=>"_blank" } %>
            </div>
            <% if ((count+1)%3)==0 %>
                <br>
            <% end %>
            <% count+=1 %>
        <% end %>
    
</div>
